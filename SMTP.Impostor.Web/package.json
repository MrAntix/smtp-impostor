{
  "name": "smtp-impostor",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "barrels": "barrelsby -q --delete --directory src --include \\.ts$ --exclude \"\\.(spec|e2e)\\.ts$\" --location all",
    "build": "stencil build",
    "build.watch": "stencil build --dev --watch",
    "icons": "node package-icons.js",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll --screenshot",
    "generate": "stencil generate"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/core": "^3.2.1",
    "@stencil/redux": "^0.2.0",
    "@stencil/router": "^1.0.1",
    "@types/jest": "^27.0.3",
    "@types/puppeteer": "5.4.7",
    "barrelsby": "^2.5.1",
    "convert-svg-to-png": "^0.6.4",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "png-to-ico": "^2.1.8",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.2",
    "workbox-build": "6.5.4"
  }
}
