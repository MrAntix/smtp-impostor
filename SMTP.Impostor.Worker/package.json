    "imagemin": "^6.1.0",
    "imagemin-pngquant": "^7.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
