    "imagemin": "^6.1.0",
    "imagemin-pngquant": "^7.0.0",
    "to-ico": "^1.1.5",
    "@types/jest": "24.0.18",
    "@types/puppeteer": "1.19.0",
    "barrelsby": "^2.1.1",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "workbox-build": "4.3.1"
  }
